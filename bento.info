ExtraSourceFiles:
    docs/*,
    docs/emacs/**/*,
    docs/source/**/*,
    docs/sphinxext/**/*,
    ipython.py,
    IPython/.git_commit_info.ini.in,
    IPython/testing/plugin/*,
    IPython/scripts/*py,
    IPython/scripts/iptest,
    IPython/scripts/ipython,
    IPython/scripts/irunner,
    IPython/scripts/pycolor,
    scripts/*.py,
    setup.py,
    setupbase.py,
    setupegg.py,
    setupext/*.py

DataFiles: man
    SourceDir: docs/man
    TargetDir: $mandir/man1
    Files: *.1

DataFiles: examples
    SourceDir: docs/examples
    TargetDir: $docdir/examples
    Files: core/*.desktop, core/*.leo, core/*py, core/*reg, vim/*,
        lib/*.py, parallel/**/*.py, parallel/**/*.ipy

DataFiles: user_profile
    TargetDir: $sitedir
    Files: IPython/config/profile/README, IPython/config/profile/**/*.py

HookFile: bscript

